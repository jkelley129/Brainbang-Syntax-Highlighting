{
  "name": "brainbang",
  "displayName": "Brainbang Syntax",
  "description": "Syntax highlighting for the brainbang language https://github.com/jkelley129/BrainBang",
  "publisher": "JoshKelley",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.0.0"
  },
    "repository": {
    "type": "git",
    "url": "https://github.com/jkelley129/Brainbang-Syntax-Highlighting.git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "brainbang",
      "aliases": ["Brainbang", "brainbang"],
      "extensions": [".bb"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "brainbang",
      "scopeName": "source.brainbang",
      "path": "./syntaxes/brainbang.tmLanguage.json"
    }]
  }
}
