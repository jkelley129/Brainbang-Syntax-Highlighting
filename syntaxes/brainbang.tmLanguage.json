{
  "scopeName": "source.brainbang",
  "name": "BrainBang",
  "patterns": [
    {
      "name": "keyword.control.brainbang",
      "match": "\\b(ent|inc|dec|cellout|loop|cellin)\\b"
    },
    {
      "name": "constant.numeric.brainbang",
      "match": "\\b\\d+\\b"
    },
    {
      "name": "keyword.operator.brainbang",
      "match": ">{1,2}\\d*"
    },
    {
      "name": "punctuation.terminator.brainbang",
      "match": ";"
    },
    {
      "name": "comment.line.double-slash.brainbang",
      "match": "//.*$"
    },
	{
  	  "name": "string.quoted.double.brainbang",
      "begin": "\"",
  	  "end": "\""
	}
  ],
  "repository": {}
}
